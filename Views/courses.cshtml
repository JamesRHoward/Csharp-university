<h3>List of course add</h3>
@if(Model.Count == 0)
{
  <p>
    No courses have been added yet.
  </p>
}
else
{
  <ul>
  @foreach(var course in Model)
  {
    <li><a href="/courses/@course.GetId()">@course.GetCourseName() @course.GetCourseNumber()</a></li>
  }
</ul>
}
<p>
  <a href="/">Home Page</a>
</p>
